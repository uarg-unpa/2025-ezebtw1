//Alumno atributos y m√©todos
public class alumno{
    private String nombre;
    private String genero;
    private int edad;
    private int dni;

    public void setNombre(String nombre){
        this.nombre = nombre;
    }
    public String getNombre(){
        return nombre;
    }
    public void setGenero(String genero){
        this.genero = genero;
    }
    public String getGenero(){
        return genero;
    }
    public void setEdad(int edad){
        this.edad = edad;
    }
    public int getEdad(){
        return edad;
    }
    public void setDni(int dni){
        this.dni = dni;
    }
    public int getDni(){
        return dni;
    }
}